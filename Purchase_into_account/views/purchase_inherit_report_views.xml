<?xml version="1.0"?>
<odoo>
    <record id="view_order_form_inherit_purchase_invoice" model="ir.ui.view">
		<field name="name">purchase.order.form.purchasereport.module</field>
		<field name="model">purchase.order.line</field>
		<field name="inherit_id" ref="purchase.purchase_order_line_form2"/>
		<field name="arch" type="xml">
			<sheet>
				<xpath expr="//menuitem[@name='menu_account_AccountMoveLine']">
				
					<field name="name"/>
					<field name="x_studio_cuenta_contable"/>
					<field name="analytic_distribution"/>
					<field name="price_total"/>
				
				</xpath>
			</sheet>
		</field>
	</record>
	
</odoo>